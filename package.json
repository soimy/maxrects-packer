{
  "name": "maxrects-packer",
  "version": "2.7.3",
  "description": "A max rectangle 2d bin packer for packing glyphs or images into multiple sprite-sheet/atlas",
  "type": "module",
  "main": "dist/maxrects-packer.js",
  "module": "dist/maxrects-packer.mjs",
  "types": "dist/maxrects-packer.d.ts",
  "scripts": {
    "clean": "rimraf ./lib && rimraf ./dist",
    "build": "rollup -c",
    "build:clean": "npm run clean && npm run build",
    "doc": "typedoc && touch docs/.nojekyll && cp assets/favicon.ico docs/ && cp assets/favicon.png docs/assets/",
    "doc:clean": "rimraf docs && mkdir docs",
    "doc:json": "typedoc --json docs/typedoc.json",
    "doc:publish": "gh-pages --nojekyll -m \"[ci skip] Updates\" -d docs",
    "doc:serve": "cd docs && npx http-server",
    "test": "npm run build:clean && jest",
    "cover": "npm run build:clean && jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "version": "standard-version",
    "prepare-release": "npm run test && npm run version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://soimy@github.com/soimy/maxrects-packer.git"
  },
  "keywords": [
    "spritesheet",
    "atlas",
    "bin",
    "pack",
    "max",
    "rect"
  ],
  "author": "YM Shen <soimy@163.com> (http://github.com/soimy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soimy/maxrects-packer/issues"
  },
  "homepage": "https://github.com/soimy/maxrects-packer#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.18.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "ascii-table": "^0.0.9",
    "c8": "^10.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.3.1",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typedoc": "^0.28.11",
    "typedoc-unhoax-theme": "^0.5.3",
    "typescript": "^5.6.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
